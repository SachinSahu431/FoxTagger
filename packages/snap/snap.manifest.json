{
  "version": "0.1.0",
  "description": "An example Snap written in TypeScript.",
  "proposedName": "FoxTagger Snap",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/template-snap-monorepo.git"
  },
  "source": {
    "shasum": "F8PaP9BuPEGCXLxOpCz6STrd9HSXQPTd22ZSRU7+T9Q=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/icon.svg",
        "packageName": "snap",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "endowment:rpc": {
      "dapps": true,
      "snaps": true
    },
    "endowment:network-access": {},
    "endowment:transaction-insight": {},
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "0 0 * * 0",
          "request": {
            "method": "weeklySummary"
          }
        },
        {
          "expression": "0 0 * * *",
          "request": {
            "method": "checkLimits"
          }
        },
        {
          "expression": "* * * * *",
          "request": {
            "method": "updateAmount"
          }
        }
      ]
    },
    "snap_notify": {},
    "snap_manageState": {}
  },
  "manifestVersion": "0.1"
}
